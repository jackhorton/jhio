{
    // environment
    "node": true,
    "browser": false,

    // enforcing
    "curly": true,
    "eqeqeq": true,
    "freeze": true,
    "maxparams": 4,
    "plusplus": false,
    "undef": true,
    "unused": "vars",
    "varstmt": true,

    // relaxing
    "esnext": true,
    "globalstrict": true,
    "laxbreak": true,
    "validthis": true
}
